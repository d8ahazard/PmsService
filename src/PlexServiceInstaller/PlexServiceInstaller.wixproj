    <ProjectGuid>5caceafa-5c9f-4a66-8fb9-dd35fb95b5cd</ProjectGuid>
    <SchemaVersion>2.0</SchemaVersion>
    <OutputName>PlexServiceInstaller</OutputName>
    <OutputType>Package</OutputType>
