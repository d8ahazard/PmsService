    </PropertyGroup>
    <PropertyGroup>
      <LinkerBaseInputPaths>..\..\src\PlexServiceTray\bin\publish</LinkerBaseInputPaths>
      <DefineConstants>PmsTrayBasePath=..\..\src\PlexServiceTray\bin\publish</DefineConstants>
